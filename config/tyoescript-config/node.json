{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 20 (LTS)",
  "_version": "20.11.0",
  "compilerOptions": {
    "lib": [
      "es2024"
    ],
    "module": "NodeNext",
    "target": "es2024",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}